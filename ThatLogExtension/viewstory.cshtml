@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Environment.GetEnvironmentVariable("WEBSITE_SITE_NAME") - Azure Web Site Log Browser - View Story</title>
    <link rel="stylesheet" href="Content/css/bootstrap.css" type="text/css" />
    <link href="Content/css/thatlog.css" rel="stylesheet" />
    <link href="Content/css/daterangepicker-bs3.css" rel="stylesheet" />

    <script src="Content/js/jquery-1.10.1.min.js"></script>
    <script src="Content/js/bootstrap.js"></script>

    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
    <script type="text/javascript" src="Content/js/angular-ui-router.min.js"></script>

    <script src="Content/js/angular-json-explorer.min.js"></script>
