    <script src="Content/js/moment.min.js"></script>
    <script src="Content/js/daterangepicker.js"></script>
    <script src="Content/js/ng-bs-daterangepicker.min.js"></script>

    <script src="Content/js/story.js"></script>
</head>
<body>
    <div ng-app="viewstory">
        <div class="container">

            <div class="navbar navbar-default navbar-fixed-top">
                <div class="container">
